{
  "name": "mstore",
  "version": "0.5.0",
  "description": "Revolutionize your shopping and selling experience with our modern marketplace app.",
  "main": "index.js",
  "scripts": {
    "start": "npx live-server . --port=3000 --host=0.0.0.0",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "npm run start",
    "emulators": "node tools/scripts/set-emulator.js && firebase emulators:start",
    "upload": "wait-on tcp:8090 tcp:9099 && node tools/scripts/upload-to-emulator.js",
    "start:dev": "wait-on tcp:8090 tcp:9099 && npm start",
    "open:ui": "wait-on http://127.0.0.1:4000 && open-cli http://127.0.0.1:4000"
  },
  "keywords": [
    "pwa",
    "local",
    "store",
    "vanilla-js"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "firebase-admin": "^13.4.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "live-server": "^1.2.2",
    "wait-on": "^9.0.1",
    "open-cli": "^8.0.0"
  }
}
